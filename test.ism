define fac x
       if = x
            0
          1
          * x
            fac - x
                  1

define -- x
       - x
         1

define range x
       if zero? x
          list x
          append range -- x
                 list x

display "A \"string\"\n"
display list #f
             (list #\) )
             #\(
             #\newline
             'symbol
             #\newline

write `(list ,(+ 1 2) 4)
group newline
write ` group list
              unquote + 1
                        2
              4
group newline
;write `#(10 5 ,(sqrt 4) ,@(map sqrt '(16 9)) 8)           

write map fac
          range 10
group newline
write fac 5
group newline
